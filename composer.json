{
    "name": "dive-be/laravel-geo",
    "description": "Translate IP addresses into geo locations",
    "keywords": [
        "dive",
        "geo",
        "location",
        "iso",
        "laravel"
    ],
    "homepage": "https://github.com/dive-be/laravel-geo",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed Sari",
            "email": "muhammed@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "geoip2/geoip2": "^2.11",
        "nunomaduro/larastan": "^0.7",
        "orchestra/testbench": "^6.13",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "geoip2/geoip2": "Required for using MaxMind / GeoIP2 database or web services."
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Dive\\Geo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config .php_cs.dist --allow-risky=yes",
        "static": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\Geo\\GeoServiceProvider"
            ],
            "aliases": {
                "Geo": "Dive\\Geo\\Facades\\Geo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
